<ul class="social-media {{class}}">
  <li class="social-media__item">
    <a class="social-media__link" data-event-name="primary-nav-social-media" data-event-value="twitter" href="{{links.twitter}}" target="_blank">
    {{> icon class="social-media__icon social-media__icon--twitter" name="twitter" twitter=true}}
    </a>
  </li>
  <li class="social-media__item">
    <a class="social-media__link" data-event-name="primary-nav-social-media" data-event-value="LinkedIn" href="{{links.linkedin}}" target="_blank">
      {{> icon class="social-media__icon social-media__icon--linkedin" name="linkedin" linkedin=true}}
    </a>
  </li>
</ul>
